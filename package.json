{
  "name": "@artesa/feathers-file-stream",
  "version": "0.7.0",
  "description": "feathers.js plugin for file uploads and downloads with streams",
  "author": "Artesa",
  "homepage": "https://github.com/Artesa/feathers-file-stream",
  "repository": {
    "type": "git",
    "url": "https://github.com/Artesa/feathers-file-stream"
  },
  "keywords": [
    "feathersjs",
    "feathers-plugin",
    "upload",
    "file",
    "stream",
    "s3"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "dist/**",
    "tsconfig.json",
    "vite.config.ts"
  ],
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "build": "shx rm -rf dist/ && vite build",
    "version": "pnpm run build",
    "release": "np",
    "test": "vitest run",
    "vitest": "vitest",
    "lint": "eslint"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.374.0",
    "@aws-sdk/client-s3": "^3.931.0",
    "@aws-sdk/lib-storage": "^3.931.0",
    "@feathersjs/errors": "^5.0.37",
    "@feathersjs/feathers": "^5.0.37",
    "express": "^4.19.2",
    "feathers-hooks-common": "^8.2.1",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@feathers-community/eslint-config": "^0.0.10",
    "@feathersjs/client": "^5.0.37",
    "@feathersjs/express": "^5.0.37",
    "@feathersjs/rest-client": "^5.0.37",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "aws-sdk-client-mock": "^4.1.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "eslint": "^9.39.1",
    "get-port": "^7.1.0",
    "helmet": "^8.1.0",
    "node-fetch": "^3.3.2",
    "np": "^10.2.0",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "supertest": "^7.1.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.9"
  }
}
